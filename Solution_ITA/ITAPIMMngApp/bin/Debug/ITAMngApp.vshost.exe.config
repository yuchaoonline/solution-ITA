<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--数据库连接字符串-->
    <add key="JtOracleConnection" value="Data Source=hycdb;User ID=hycdb;Password=hycdb;"/>
    <add key="IsofReadiniConnection" value="true"/>
    <!--数据库连接是否加密-->
    <add key="ConStringEncrypt" value="false"/>
    <!--DLL的文件名称-->
    <add key="CARDAL" value="Djdw.OracleDAL"/>
    <!--数据库访问中四个命名空间-->
    <add key="SYSDAL" value="Djdw.OracleDAL"/>
    <add key="MIPDAL" value="Mis.OracleDAL"/>
    <add key="gtled" value="gtled.OracleDAL"/>
    <add key="xxtyDAL" value="xxty.OracleDAL"/>
    <add key="scsoftDAL" value="hbapp.OracleDAL"/>
    <add key="xfsoftDAL" value="xfapp.OracleDAL"/>

    <!--基础管理平台的地址-->
    <add key="MIPUrlPath" value="http://localhost:3785/BaseMngWebApp/LoginDJ.aspx"/>

    <!--正则表达式验证输入信息-->
    <add key="RegDoubleMatch" value="^(-?\d+)(\.\d+)?$"/>
    <add key="RegNotNegtiveDoubleMatch" value="^\d+(\.\d+)?$"/>
    <add key="RegPositiveDoubleMatch" value="(([0-9]+\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\.[0-9]+)|([0-9]*[1-9][0-9]*))$"/>
    <add key="RegNotPositiveDouble" value="^((-\d+(\.\d+)?)|(0+(\.0+)?))$"/>
    <add key="RegNegtiveDoubleMatch" value="^(-(([0-9]+\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\.[0-9]+)|([0-9]*[1-9][0-9]*)))$"/>

    <add key="RegIntMatch" value="^-?\d+$"/>
    <add key="RegNotNegtiveIntMatch" value="^\d+$"/>
    <add key="RegPositiveIntMatch" value="^[0-9]*[1-9][0-9]*$"/>
    <add key="RegNotPositiveMatch" value="^((-\d+)|(0+))$"/>
    <add key="RegNegtiveIntMatch" value="^-[0-9]*[1-9][0-9]*$"/>
    
    <add key="RegLettersMatch" value="^[A-Za-z]+$"/>
    <add key="RegUperLettersMatch" value="^[A-Z]+$"/>
    <add key="RegLowerLettersMatch" value="^[a-z]+$"/>
    <add key="RegLettersANDNumbersMatch" value="^[A-Za-z0-9]+$"/>
    <add key="RegLettersANDNumbersANDDownlinesMatch" value="^\w+$"/>
    
    <add key="RegYYMMDDMatch" value="/^(d{2}|d{4})-((0([1-9]{1}))|(1[1|2]))-(([0-2]([1-9]{1}))|(3[0|1]))$/"/>
    <add key="RegPhoneNumberMatch" value="(d+-)?(d{4}-?d{7}|d{3}-?d{8}|^d{7,8})(-d+)?"/>
    <add key="RegIPAddressMatch" value="^(d{1,2}|1dd|2[0-4]d|25[0-5]).(d{1,2}|1dd|2[0-4]d|25[0-5]).(d{1,2}|1dd|2[0-4]d|25[0-5]).(d{1,2}|1dd|2[0-4]d|25[0-5])$"/>
  </appSettings>
</configuration>